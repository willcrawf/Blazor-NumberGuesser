{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
        "version": "0.2.0",
        "configurations": [   
            {
                "name": "Debug",
                "type": "coreclr",
                "request": "launch",
                "preLaunchTask": "Debug-Build",            
                "program": "dotnet",
                "args": ["run", "${workspaceFolder}/bin/Debug/net7.0/SyncfusionBlazorApp.dll"],
                "cwd": "${workspaceFolder}",
                "stopAtEntry": false,
                "internalConsoleOptions": "openOnSessionStart",
                "launchBrowser": {
                    "enabled": true
                },
                "env": {
                    "ASPNETCORE_ENVIRONMENT": "Development"
                },
                "sourceFileMap": {
                    "/Views": "${workspaceFolder}/Views"
                }
            },
			{
                "name": "Release",
                "type": "coreclr",
                "request": "launch",
                "preLaunchTask": "Release-Build",            
                "program": "dotnet",
                "args": ["run", "${workspaceFolder}/bin/Release/net7.0/SyncfusionBlazorApp.dll"],
                "cwd": "${workspaceFolder}",
                "stopAtEntry": false,
                "internalConsoleOptions": "openOnSessionStart",
                "launchBrowser": {
                    "enabled": true
                },
                "env": {
                    "ASPNETCORE_ENVIRONMENT": "Development",
                },
                "sourceFileMap": {
                    "/Views": "${workspaceFolder}/Views"
                }
            }
        ]
    }
    